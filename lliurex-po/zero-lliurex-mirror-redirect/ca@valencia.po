#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:114
msgid "You need to be on a slave server to run mirror-redirect"
msgstr "Heu d'estar en un servidor esclau per a executar aquest programa."

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:123
msgid "Mirror Redirect"
msgstr "Redirecció de la rèplica"

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:136
msgid ""
"From here you can redirect the mirror in a slave server to the master "
"server's own mirror"
msgstr ""
"Des d'ací es pot redireccionar el mirror d'un server esclau per a que "
"utilitze el mirror del server mestre."

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:137
msgid "Redirect mirror"
msgstr "Redireccionar el mirror"

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:145
msgid "Enable mirror redirection"
msgstr "Activar la redirecció del mirror"

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:162
#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:208
msgid "Using master server mirror"
msgstr "S'està utilitzant la rèplica del servidor mestre"

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:164
#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:210
msgid "Using local mirror"
msgstr "S'està utilitzant la rèplica local"

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:195
#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:199
msgid "Redirecting mirror..."
msgstr "S'està redirigint la rèplica..."

#: ../zero-lliurex-mirror-redirect.install/usr/sbin/mirror-redirect.py:231
msgid "You need root privileges to run mirror-redirect"
msgstr ""
"Heu de tindre privilegis d'usuari primari per a executar aquesta aplicació"

#~ msgid "Mirror redirect"
#~ msgstr "Redirecció de la rèplica"

#~ msgid "Enable Mirror Redirect"
#~ msgstr "Habilita la redirecció de la rèplica"
